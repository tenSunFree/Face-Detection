<?xml version="1.0" encoding="utf-8"?>

<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item 
        android:state_pressed="false">
        <shape>
            <solid android:color="#FF9224" />
            <corners android:radius="10dp" />
            <stroke 
                android:width="10px" 
                android:color="#DDD"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#FF8F16" />
            <corners android:radius="10dp" />
            <stroke 
                android:width="10px" 
                android:color="#DDD"/>
        </shape>
    </item>
</selector>
